{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["error", "double", {
      "avoidEscape": true
    }],
    "indent": ["error", 2],
    "@typescript-eslint/no-unused-vars": ["error", { "args": "none" }]
  }
}
